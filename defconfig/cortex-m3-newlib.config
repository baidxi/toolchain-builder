
export ARCH=arm
export TARGET=arm-linux-eabi
export HOST=x86_64-w64-mingw32

export VARIANT=cortex-m3-newlib
export PROGRAM_PREFIX=arm-linux
export TARGET_GCC_FLAGS=--with-float=soft --with-arch=armv7-m --with-tune=cortex-m3 --with-abi=aapcs-linux

export LIBC=newlib
export LIBC_USE_SOFT_FLOAT=y
export LIBC_DEBUG=

TARGET_CFLAGS += -Os
export TARGET_CFLAGS

