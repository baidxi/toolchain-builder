
export ARCH=x86
export TARGET=i686-pc-linux-gnu
export HOST=x86_64-w64-mingw32

export VARIANT=i686-glibc
export PROGRAM_PREFIX=i686-linux-gnu
export TARGET_GCC_FLAGS=--with-default-libstdcxx-abi=new --enable-gnu-unique-object --disable-vtable-verify --enable-default-pie -with-abi=m32 --with-tune=generic

export LIBC=glibc
export LIBC_USE_SOFT_FLOAT=
export LIBC_DEBUG=

TARGET_CFLAGS += -O2
export TARGET_CFLAGS

