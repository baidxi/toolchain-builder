
export ARCH=arm
export TARGET=arm-linux-uclibceabi
export HOST=x86_64-w64-mingw32

export VARIANT=arm926ejs-uclibc
export PROGRAM_PREFIX=arm-linux
export TARGET_GCC_FLAGS=--with-float=soft --with-arch=armv5te --with-tune=arm926ej-s --with-abi=aapcs-linux

export LIBC=uclibc-ng
export LIBC_USE_SOFT_FLOAT=y
export LIBC_DEBUG=

TARGET_CFLAGS += -Os
export TARGET_CFLAGS

